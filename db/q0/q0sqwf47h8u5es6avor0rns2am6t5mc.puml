@startuml

!function $LambdaPNG($scale=1)
!return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction

!function $LambdaSVG($scale=1)
!return "<img data:image/svg+xml;base64,{scale="+$scale+"}>"
!endfunction

hide footbox

actor Developer as user
participant "$LambdaPNG()%n()Lambda" as lambdaPNG
participant "$LambdaSVG()%n()Lambda" as lambdaSVG
participant "$LambdaPNG(2)%n()Lambda" as lambdaPNG2
participant "$LambdaSVG(2)%n()Lambda" as lambdaSVG2

user -> lambdaPNG: PNG
user -> lambdaSVG: SVG
user -> lambdaPNG2: PNG {scale=2}
user -> lambdaSVG2: SVG {scale=2}
@enduml
